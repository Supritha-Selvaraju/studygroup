<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PHP-FastCGI" path="*.php" verb="GET,POST,HEAD" modules="FastCgiModule"
           scriptProcessor="D:\home\site\wwwroot\php\php-cgi.exe" resourceType="Either" requireAccess="Script" />
    </handlers>
  </system.webServer>
</configuration>
